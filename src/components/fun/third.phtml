<?php

namespace Fun;

use Ephect\Template\TemplateEngine;

use function Ephect\Hooks\useEffect;

function Third()
{
    $code = '';
    useEffect(function() use (&$code) {
        $engine = new TemplateEngine('Child');
        $dictionary = ["firstname" => "David"];
        $code = $engine->render($dictionary);
    });

    return (<<<HTML
        {{ code }}
    HTML);
}